<html>
<head>
  <link rel="stylesheet" href="/css/master.css">
</head>
<body>
  <h1>{{title}}</h1>

  <div class="profile">

    {{#person}}
      <div class="imageDiv">
        <img src={{avatar}} alt="">
      </div>
      <div class="profileInfo">
          <h1>{{name}}</h1>
          <p>{{address.city}}, {{address.country}}</p>
          <p>{{email}} | {{phone}}</p>
      </div>
      </div>
      <div class="bottomInfo">
          <p style="font-weight: bold;"> Education:</p><p>{{university}}</p><br>
      </div>
      <div class="bottomInfo2">
          <p style="font-weight: bold;"> Skills:</p> <p>{{skills}}</p>
      </div>

    {{/person}}


</body>
</html>
